#include <stdio.h>

void main() {
    float kilograms, grams;

    // Taking input
    printf("Enter weight in kilograms: ");
    scanf("%f", &kilograms);

    // Conversion (1 kg = 1000 g)
    grams = kilograms * 1000;

    // Display result
    printf("Weight in grams = %.2f g\n", grams);
}


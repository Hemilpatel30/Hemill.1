#include <stdio.h>

int main() {
    float fahrenheit, celsius;

    // Taking input
    printf("Enter temperature in Fahrenheit: ");
    scanf("%f", &fahrenheit);

    // Conversion formula
    celsius = (5.0 / 9.0) * (fahrenheit - 32);

    // Display result
    printf("%.2f Fahrenheit = %.2f Celsius\n", fahrenheit, celsius);

    return 0;
}


#include <stdio.h>

int main() {
    float side, area, perimeter;

    // Taking input
    printf("Enter side length of the square: ");
    scanf("%f", &side);

    // Formula calculations
    area = side * side;
    perimeter = 4 * side;

    // Display results
    printf("Area of Square = %.2f\n", area);
    printf("Perimeter of Square = %.2f\n", perimeter);

    return 0;
}
